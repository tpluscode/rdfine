{
  "name": "rdfine",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts,.js --quiet --ignore-path .eslintignore --ignore-path .gitignore",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "example": "wsrun -p rdfine-example --no-prefix -c start",
    "generate": "npm run generate:one",
    "generate:one": "wsrun --no-prefix -rltm -p $npm_config_package -c generate && echo",
    "generate:all": "wsrun --no-prefix -rltm -c generate",
    "clean": "wsrun --no-prefix -rltm -c clean",
    "build": "wsrun -cm prepack",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.10.0",
    "@rdfjs/parser-jsonld": "^2.1.0",
    "@rdfjs/types": "*",
    "@tpluscode/eslint-config": "^0.4.2",
    "@types/clownface": "^1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18",
    "@types/rdfjs__parser-jsonld": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^29.5.0",
    "jest-snapshot": "^29.5.0",
    "lint-staged": "^10.2.12",
    "npm-run-all": "^4.1.5",
    "rdf-ext": "^2.2.0",
    "rimraf": "^3.0.2",
    "standard": "^17.0.0",
    "string-to-stream": "^3.0.1",
    "ts-jest": "^29.1.0",
    "ts-morph": "^7.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "wsrun": "^5.2.4"
  },
  "workspaces": [
    "examples",
    "packages/*",
    "vocabularies/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  }
}
