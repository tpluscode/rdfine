{
  "name": "rdfine",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.js --quiet --ignore-path .eslintignore --ignore-path .gitignore",
    "test": "run-s test:*",
    "test:ts-node": "jest --coverage",
    "test:babel": "jest --coverage -c jest.babel.config.js",
    "example": "wsrun -p rdfine-example --no-prefix -c start",
    "generate": "npm run generate:one",
    "generate:one": "wsrun --no-prefix -rltm -p $npm_config_package -c generate && echo",
    "generate:all": "wsrun --no-prefix -rltm -c generate",
    "clean": "wsrun --no-prefix -rltm -c clean",
    "build": "wsrun -cm prepack",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.10.0",
    "@rdfjs/parser-jsonld": "^1.3.0",
    "@tpluscode/eslint-config": "0.1.1",
    "@types/clownface": "^1",
    "@types/jest": "^26.0.3",
    "@types/node": "^12",
    "@rdfjs/types": "*",
    "@types/rdfjs__parser-jsonld": "^1.2.3",
    "@types/rdf-dataset-indexed": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^27.3.0",
    "jest-snapshot": "^27.3.0",
    "lint-staged": "^10.2.12",
    "npm-run-all": "^4.1.5",
    "rdf-ext": "^1.3.0",
    "rimraf": "^3.0.2",
    "standard": "^14.3.1",
    "string-to-stream": "^3.0.1",
    "ts-jest": "^27.0.7",
    "ts-morph": "^7.1.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.4",
    "wsrun": "^5.2.4"
  },
  "workspaces": [
    "examples",
    "packages/*",
    "vocabularies/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  }
}
