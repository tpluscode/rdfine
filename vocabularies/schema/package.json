{
  "name": "@rdfine/schema",
  "version": "0.8.3",
  "main": "index.js",
  "module": "index.js",
  "browser": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./Factory": "./Factory.js",
    "./bundles": "./bundles/index.js",
    "./lib/*": "./lib/*.js",
    "./extensions/*": "./extensions/*.js"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "tsc",
    "generate": "rdf-vocab prefix schema | node --loader ts-node/esm ../../packages/generator/bin/rdfine-gen.ts --format application/n-quads "
  },
  "rdfine": {
    "prefix": "schema",
    "types": {
      "schema:CssSelectorType": "string",
      "schema:URL": "NamedNode",
      "schema:XPathType": "string"
    },
    "properties": {
      "itemListElement": {
        "values": "array"
      },
      "step": {
        "values": "array"
      },
      "steps": {
        "values": "array"
      },
      "tool": {
        "values": "array"
      }
    },
    "exclude": [
      "Class",
      "DataType",
      "DefinedTerm",
      "EducationalOccupationalCredential",
      "Enumeration",
      "GeospatialGeometry",
      "MedicalBusiness",
      "VirtualLocation"
    ]
  },
  "dependencies": {
    "@rdfjs/data-model": "^2",
    "@tpluscode/rdf-ns-builders": ">=3.0.2",
    "@tpluscode/rdfine": "^0.7.3"
  },
  "devDependencies": {
    "@rdfine/test-support": "*",
    "@zazuko/env": "^1.3.1",
    "chai": "^4.3.7",
    "clownface": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/schema"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/schema#readme"
}
