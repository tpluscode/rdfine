{
  "name": "@rdfine/schema",
  "version": "0.6.1",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./bundles": {
      "require": "./bundles/index.js",
      "import": "./bundles/index.mjs"
    },
    "./lib/*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    },
    "./extensions/*": {
      "require": "./extensions/*.js",
      "import": "./extensions/*.mjs"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:mjs": "BABEL_ENV=modules babel . --out-dir . --extensions .ts --out-file-extension .mjs --config-file ../../babel.config.json",
    "generate": "rdf-vocab prefix schema | rdfine-gen --format application/n-quads --dev"
  },
  "rdfine": {
    "prefix": "schema",
    "types": {
      "schema:CssSelectorType": "string",
      "schema:URL": "NamedNode",
      "schema:XPathType": "string"
    },
    "properties": {
      "itemListElement": {
        "values": "array"
      },
      "step": {
        "values": "array"
      },
      "steps": {
        "values": "array"
      },
      "tool": {
        "values": "array"
      }
    },
    "exclude": [
      "Class",
      "DataType",
      "DefinedTerm",
      "EducationalOccupationalCredential",
      "Enumeration",
      "GeospatialGeometry",
      "MedicalBusiness",
      "VirtualLocation"
    ]
  },
  "dependencies": {
    "@rdf-esm/data-model": "^0.5.3",
    "@tpluscode/rdf-ns-builders": "^0.4",
    "@tpluscode/rdfine": "^0.5.17"
  },
  "peerDependencies": {
    "@types/rdf-js": ">3"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^1.2.0",
    "@rdfjs/dataset": "^1.0.1",
    "clownface": "^1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/schema"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/schema#readme"
}
