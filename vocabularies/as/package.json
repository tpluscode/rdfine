{
  "name": "@rdfine/as",
  "version": "0.4.0",
  "main": "index.js",
  "module": "index.js",
  "browser": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./bundles": "./bundles/index.js",
    "./lib/*": "./lib/*.js",
    "./extensions/*": "./extensions/*.js"
  },
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "tsc",
    "generate": "rdf-vocab prefix as | node --loader ts-node/esm ../../packages/generator/bin/rdfine-gen.ts --format application/n-quads"
  },
  "rdfine": {
    "prefix": "as",
    "exclude": [
      "as:id"
    ]
  },
  "dependencies": {
    "@rdfjs/data-model": "^2",
    "@tpluscode/rdf-ns-builders": ">=3.0.2",
    "@tpluscode/rdfine": "^0.7.0",
    "@rdfine/rdf": "^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/as"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph",
    "activitystreams"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/as#readme"
}
