{
  "name": "@rdfine/shacl",
  "version": "0.7.3",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./bundles": {
      "require": "./bundles/index.js",
      "import": "./bundles/index.mjs"
    },
    "./lib/*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    },
    "./extensions/*": {
      "require": "./extensions/*.js",
      "import": "./extensions/*.mjs"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:mjs": "BABEL_ENV=modules babel . --out-dir . --extensions .ts --out-file-extension .mjs --config-file ../../babel.config.json",
    "generate": "{ rdf-vocab prefix sh ; cat ./overrides.nq ; } | rdfine-gen --format application/n-quads --dev"
  },
  "rdfine": {
    "prefix": "sh",
    "properties": {
      "hasValue": {
        "values": "array"
      },
      "languageIn": {
        "values": "list"
      },
      "and": {
        "values": "list"
      },
      "or": {
        "values": "list"
      },
      "xone": {
        "values": "list"
      },
      "in": {
        "values": "list"
      },
      "property": {
        "values": "array"
      },
      "targetClass": {
        "values": "array"
      },
      "targetNode": {
        "values": "array"
      },
      "path": {
        "values": [
          "list",
          "single"
        ]
      }
    },
    "exclude": [
      "rdf:List"
    ]
  },
  "dependencies": {
    "@rdf-esm/data-model": "^0.5.3",
    "@rdfine/owl": "^0.4.3",
    "@rdfine/rdf": "^0.5.3",
    "@rdfine/rdfs": "^0.6.3",
    "@tpluscode/rdf-ns-builders": "^0.4",
    "@tpluscode/rdfine": "^0.5.17"
  },
  "peerDependencies": {
    "@types/rdf-js": ">3"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^1.2",
    "@rdfjs/dataset": "^1.0.1",
    "clownface": "^1",
    "rdf-ext": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/shacl"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/csvw#readme"
}
