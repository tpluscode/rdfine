{
  "name": "@rdfine/doap",
  "version": "0.0.1",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./bundles": {
      "require": "./bundles/index.js",
      "import": "./bundles/index.mjs"
    },
    "./lib/namespace": {
      "require": "./lib/namespace.js",
      "import": "./lib/namespace.mjs"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "clean": "find . -name '*.ts' -maxdepth 1 -delete",
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:mjs": "BABEL_ENV=modules babel . --out-dir . --extensions .ts --out-file-extension .mjs --config-file ../../babel.config.json",
    "generate": "rdf-vocab prefix doap | node ../../packages/generator/bin/rdfine-gen.js --format application/n-quads"
  },
  "rdfine": {
    "prefix": "doap",
    "types": {
      "http://rdfs.org/sioc/types#Weblog": "NamedNode",
      "http://rdfs.org/sioc/types#MailingList": "NamedNode",
      "http://rdfs.org/sioc/types#Wiki": "NamedNode"
    }
  },
  "dependencies": {
    "@rdf-esm/data-model": "^0.5.3",
    "@rdfine/foaf": "*",
    "@rdfine/rdfs": "*",
    "@rdfine/sioc": "*",
    "@tpluscode/rdf-ns-builders": "^0.4",
    "@tpluscode/rdfine": "^0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/doap"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/doap#readme"
}
