{
  "name": "@rdfine/rico",
  "version": "0.3.2",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "browser": "index.js",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./Factory": "./Factory.js",
    "./bundles": "./bundles/index.js",
    "./lib/*": "./lib/*.js",
    "./extensions/*": "./extensions/*.js"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "tsc",
    "generate": "rdf-vocab prefix rico | node --loader ts-node/esm ../../packages/generator/bin/rdfine-gen.ts --format application/n-quads"
  },
  "rdfine": {
    "prefix": "rico"
  },
  "dependencies": {
    "@rdfjs/data-model": "^2",
    "@tpluscode/rdf-ns-builders": ">=3.0.2",
    "@tpluscode/rdfine": "^0.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git",
    "directory": "vocabularies/rico"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rico",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/rico#readme"
}
