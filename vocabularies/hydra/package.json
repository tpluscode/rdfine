{
	"name": "@rdfine/hydra",
	"version": "0.3.0",
	"main": "inde.js",
	"module": "index.mjs",
	"browser": "index.mjs",
	"types": "index.d.ts",
	"exports": {
		"./": {
			"require": "./index.js",
			"import": "./index.mjs"
		},
		"./bundles": {
			"require": "./bundles/index.js",
			"import": "./bundles/index.mjs"
		}
	},
	"scripts": {
		"prepack": "npm run build",
		"clean": "find . -name '*.ts' -maxdepth 1 -delete",
		"build": "run-s build:*",
		"build:cjs": "tsc -p tsconfig.build.json",
		"build:mjs": "BABEL_ENV=modules babel . --out-dir . --extensions .ts --out-file-extension .mjs --config-file ../../babel.config.json",
		"generate": "rdf-vocab prefix hydra | node ../../packages/generator/bin/rdfine-gen.js --format application/n-quads"
	},
	"rdfine": {
		"prefix": "hydra",
		"properties": {
			"collection": {
				"values": "array"
			},
			"manages": {
				"values": "array"
			},
			"mapping": {
				"values": "array"
			},
			"member": {
				"values": "array"
			},
			"operation": {
				"values": "array"
			},
			"possibleStatus": {
				"values": "array"
			},
			"supportedClass": {
				"values": "array"
			},
			"supportedOperation": {
				"values": "array"
			},
			"supportedProperty": {
				"values": "array"
			},
			"view": {
				"values": "array"
			}
		}
	},
	"dependencies": {
		"@rdfine/rdf": "^0.3.0",
		"@rdfine/rdfs": "^0.3.0",
		"@rdfjs/namespace": "^1.1.0",
		"@tpluscode/rdf-ns-builders": "^0.3.6",
		"@tpluscode/rdfine": "^0.4.16"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/plugin-proposal-optional-chaining": "^7.8.3",
		"@babel/preset-env": "^7.8.3",
		"@babel/preset-typescript": "^7.8.3",
		"@rdfine/generator": "^0.3.0",
		"@rdfjs/dataset": "^1.0.1",
		"@types/clownface": "^0.12.13",
		"@types/rdf-js": "^3",
		"@zazuko/rdf-vocabularies": "^2020.6.29",
		"clownface": "^0.12.1",
		"eslint": "^6.8.0",
		"jest": "^26.1.0",
		"npm-run-all": "^4.1.5",
		"ts-jest": "^26.1.1",
		"typescript": "next"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tpluscode/rdfine.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"rdf",
		"rdfjs",
		"entity",
		"decorator",
		"clownface",
		"graph"
	],
	"author": "tpluscode",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tpluscode/rdfine/issues"
	},
	"homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/csvw#readme"
}
