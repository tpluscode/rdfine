{
  "name": "@rdfine/hydra",
  "version": "0.6.0-next.0",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./bundles": {
      "require": "./bundles/index.js",
      "import": "./bundles/index.mjs"
    },
    "./lib/*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf lib bundles",
    "build": "run-s build:*",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:mjs": "BABEL_ENV=modules babel . --out-dir . --extensions .ts --out-file-extension .mjs --config-file ../../babel.config.json",
    "generate": "{ rdf-vocab prefix hydra ; cat ./overrides.nq ; } | node ../../packages/generator/bin/rdfine-gen.js --format application/n-quads"
  },
  "rdfine": {
    "prefix": "hydra",
    "properties": {
      "collection": {
        "values": "array"
      },
      "expects": {
        "values": "array"
      },
      "manages": {
        "values": "array"
      },
      "mapping": {
        "values": "array"
      },
      "member": {
        "values": "array"
      },
      "operation": {
        "values": "array"
      },
      "possibleStatus": {
        "values": "array"
      },
      "supportedClass": {
        "values": "array"
      },
      "supportedOperation": {
        "values": "array"
      },
      "supportedProperty": {
        "values": "array"
      },
      "view": {
        "values": "array"
      }
    }
  },
  "dependencies": {
    "@rdfine/rdf": "^0.5.0-next.0",
    "@rdfine/rdfs": "^0.6.0-next.0",
    "@rdf-esm/data-model": "^0.5.3",
    "@tpluscode/rdf-ns-builders": "^0.4",
    "@tpluscode/rdfine": "^0.6.0-next.0",
    "es6-url-template": "^3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rdfine/generator": "^0.5.0-next.0",
    "@rdfjs/data-model": "^1.2",
    "@rdfjs/dataset": "^1.0.1",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/parser-n3": "^1.1.4",
    "@tpluscode/rdf-string": "^0.2.20",
    "@types/clownface": "^1",
    "@types/rdf-js": "^4",
    "@zazuko/rdf-vocabularies": "^2020.6.29",
    "clownface": "^1",
    "eslint": "^6.8.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "rdf-ext": "^1.3.0",
    "string-to-stream": "^3.0.1",
    "ts-jest": "^26.1.1",
    "typescript": "next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/rdfine.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "entity",
    "decorator",
    "clownface",
    "graph"
  ],
  "author": "tpluscode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/rdfine/issues"
  },
  "homepage": "https://github.com/tpluscode/rdfine/tree/master/vocabularies/csvw#readme"
}
